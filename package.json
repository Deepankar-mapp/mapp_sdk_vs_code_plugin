{
    "name": "mapp-flutter-analyzer",
    "displayName": "Mapp Flutter SDK Assistant",
    "description": "AI-powered Mapp Flutter SDK implementation analyzer and assistant",
    "version": "1.0.16",
    "publisher": "deepankarmathur",
    "engines": {
        "vscode": "^1.75.0"
    },
    "categories": [
        "Programming Languages",
        "Linters",
        "Other"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "compile-tests": "tsc -p ./tsconfig.test.json",
        "watch": "tsc -w -p ./",
        "pretest": "npm run compile-tests",
        "test": "node ./out/test/runTest.js",
        "lint": "eslint src --ext ts",
        "package": "vsce package",
        "build": "vsce package",
        "clean": "rimraf out"
    },
    "dependencies": {
        "@babel/parser": "^7.x.x",
        "@babel/traverse": "^7.x.x",
        "@babel/types": "^7.x.x",
        "openai": "^4.0.0"
    },
    "devDependencies": {
        "@types/babel__traverse": "^7.x.x",
        "@types/babel__parser": "^7.x.x",
        "@types/node": "^16.x.x",
        "@types/vscode": "^1.60.0",
        "@typescript-eslint/eslint-plugin": "^5.x.x",
        "@typescript-eslint/parser": "^5.x.x",
        "@vscode/test-electron": "^2.x.x",
        "@vscode/vsce": "^2.x.x",
        "eslint": "^8.x.x",
        "rimraf": "^3.x.x",
        "typescript": "^4.x.x",
        "glob": "^7.2.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "mocha": "^9.2.0"
    },
    "icon": "mapp-logo.jpg",
    "contributes": {
        "commands": [
            {
                "command": "mapp-analyzer.analyze",
                "title": "Analyze Mapp Code"
            },
            {
                "command": "mapp-analyzer.getHelp",
                "title": "Get Mapp SDK Help"
            }
        ],
        "configuration": {
            "title": "Mapp SDK Analyzer",
            "properties": {
                "mappAnalyzer.openAIKey": {
                    "type": "string",
                    "default": "",
                    "description": "OpenAI API Key for AI-powered assistance"
                }
            }
        }
    }
}